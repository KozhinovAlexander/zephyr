config CORDIC_STM32
	bool "STM32 CORDIC driver"
	default y
	depends on DT_HAS_ST_STM32_CORDIC_ENABLED
	select DSP
	select CMSIS_DSP
	select CMSIS_DSP_BASICMATH
	help
	  Enable STM32 CORDIC driver.
